# NextCloud and Elasticsearch Ingestor Environment Variables
# Fill in the values as appropriate for your environment

NEXTCLOUD_URL=http://localhost:8080
NEXTCLOUD_ADMIN_USERNAME=admin
NEXTCLOUD_ADMIN_PASSWORD=adminpassword
NEXTCLOUD_DOSSIER_PARENT_PATH=Dossiers

# ELASTICSEARCH_URL=http://localhost:9200
ELASTICSEARCH_URL=http://bsw-elasticsearch:9200
# ELASTICSEARCH_INDEX_DOCUMENTS=document-index
ELASTICSEARCH_INDEX_DOCUMENTS=bsw-index
ELASTICSEARCH_INDEX_DOSSIERS=dossier-index

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=nextcloud_ingestor
POSTGRES_USER=ingestor
POSTGRES_PASSWORD=ingestor_password

# Tika Server Configuration
TIKA_SERVER_URL=http://localhost:9998

NC_URL=http://localhost:8080
NC_USER=admin
NC_PASSWORD=adminpassword
NC_DOSSIER_PARENT_PATH=Dossiers

# ES_URL=http://localhost:9200
ES_URL=http://bsw-elasticsearch:9200
ES_INDEX_DOCUMENTS=bsw-index
ES_INDEX_DOSSIERS=dossier-index
# ES_USER=elastic
# ES_PASSWORD=changeme

DRY_RUN=false